namespace std
{
extern public void print(string msg){
DX := msg;
AH := 9b;
interrupt 0x21;
}
interrupt 0
	{
		print("Interrupt 0 $");
	}
interrupt 3
	{
		print("Interrupt 3 $");
	}
extern public void putc(byte c){
DX:=c;
AH:=2b;
interrupt 0x21;
}
extern public byte getc(){
byte c = 0;
AH := 0;
interrupt 0x16;
c := AL;
AH := 0x0E;

interrupt 0x10;

   return c;
}

}
//END